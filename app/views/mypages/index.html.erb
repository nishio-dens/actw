<div class="row mt-10">

  <%= render 'shared/flash_message' %>

  <div class="col-md-9"> <!-- main -->
    <% @filters.each do |filter| %>
    <div class="actw-filters">
    <div class="panel panel-default actw-panel">
      <div class="panel-heading">
        <h2> <%= filter.name %> (0) </h2>
      </div>
      <ul class="list-group filter-products" data-filter-id="<%= filter.id %>"><!-- list-group -->
        <div class="text-center spinner">
          <i class="fa fa-spinner fa-spin"></i>
        </div>
      </ul> <!-- /list-group -->
    </div>
    <nav>
      <ul class="pagination">
        <li><a href="#"><span aria-hidden="true">&laquo;</span><span class="sr-only">Previous</span></a></li>
        <li><a href="#">1</a></li>
        <li><a href="#">2</a></li>
        <li><a href="#">3</a></li>
        <li><a href="#">4</a></li>
        <li><a href="#">5</a></li>
        <li><a href="#"><span aria-hidden="true">&raquo;</span><span class="sr-only">Next</span></a></li>
      </ul>
    </nav>
    </div>
    <% end %><!-- /filter -->

    <div class="panel panel-default actw-panel">
      <div class="panel-heading">
        <h2> ユーザ定義 リポジトリ(3)</h2>
      </div>
      <ul class="list-group"><!-- list-group -->
      <% 3.times do %>
      <li class="list-group-item">

      <div class="row"> <!-- title -->
        <div class="col-md-9">
        <%= link_to '#' do %>
          <h3> <i class="fa fa-bookmark"></i> actw-app</h3>
        <% end %>
        </div>
        <div class="col-md-3 text-right page-default-color">
          <i class="fa fa-clock-o"></i> 2014年12月07日 公開
        </div>
      </div> <!-- /title -->

      <div class="row"><!-- tag -->
        <div class="col-md-12">
        <span class="label label-info">AWS</span>
        <span class="label label-info">Ruby</span>
        <span class="label label-info">Rails</span>
        </div>
      </div>

      <div class="row"><!-- summary -->
        <div class="col-md-12 mt-10 page-default-color">
        </div>
      </div>

      <div class="row"><!-- edit -->
        <div class="col-md-12 text-right">
          <a href="#" class="">
            <i class="fa fa-pencil"></i> 編集する
          </a>
          <a href="#" class="">
            <i class="fa fa-times"></i> 削除する
          </a>
        </div>
      </div>

      </li>
      <% end %>
      </ul> <!-- /list-group -->
    </div>

    <hr>


    <h3><i class="fa fa-rocket"></i> テストユーザさんの活動</h3>

    <div class="panel panel-default actw-panel">
      <ul class="list-group"><!-- list-group -->
      <% 10.times do %>
      <li class="list-group-item">

      <div class="row">
        <div class="col-md-1 text-right">
        <%= link_to '#' do %>
          <img alt="32x32" data-src="holder.js/64x64" style="width: 32px; height: 32px;" src="data:image/svg+xml;base64," data-holder-rendered="true">
