<script id="productTmpl" type="text/x-jsrender">
<li class="list-group-item">
{{for product}}
<div class="row"> <!-- title -->
  <div class="col-md-9">
    <a href="{{:url}}">
      <h3> <i class="fa fa-bookmark"></i> {{:title}} </h3>
    </a>
  </div>
  <div class="col-md-3 text-right page-default-color">
    <i class="fa fa-clock-o"></i> {{:display_published_at}} 公開
  </div>
</div> <!-- /title -->

<div class="row"><!-- tag -->
  <div class="col-md-12">
    {{for tags}}
    <span class="label label-info">{{:#data}}</span>
    {{/for}}
  </div>
</div>

<div class="row"><!-- summary -->
  <div class="col-md-12 mt-10 page-default-color">
    {{:description}}
  </div>
</div>

<div class="row"><!-- edit -->
  <div class="col-md-12 text-right">
    <a href="#" class="mr-10">
      <i class="fa fa-pencil"></i> 編集する
    </a>
    <a href="#" class="">
      <i class="fa fa-times"></i> 削除する
    </a>
  </div>
</div>

</li>
{{/for}}
</script>
