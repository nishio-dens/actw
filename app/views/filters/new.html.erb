  <div class="col">
    <h1>フィルターを追加</h1>
  </div>
  </div><!-- /page-header -->

  <div class="col">
  <div class="cell">
  <%= simple_form_for(@form, url: filters_path, method: :post, wrapper: :cascade) do |f| %>
    <%= f.input :name %>
    <%= f.input :display_mypage, as: :boolean %>

    <!-- button -->
    <div class="col">
    <div class="cell">
      <div class="col width-1of6">
        <div class="cell"></div>
      </div>
      <div class="col width-fill">
        <div class="actw-submit-button cell">
          <%= button_tag '追加する', type: :submit %>
        </div>
      </div>
    </div>
    </div>
    <!-- button -->
  <% end %>
  </div>
  </div>
</div>

